{
  "name": "nuxt-module-template",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.6.5",
  "scripts": {
    "build": "rimraf packages/*/dist && turbo run build --filter=@hebilicious/my-module",
    "turbo:build": "turbo run build",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "pnpm lint:fix && bumpp -r -x \"pnpm run changelog\" --all",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.4",
    "@hebilicious/eslint-config": "0.0.3-beta.1",
    "@nuxt/test-utils": "^3.6.1",
    "@types/node": "^18.16.18",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "8.43.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "lint-staged": "^13.2.3",
    "pnpm": "8.6.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.3",
    "turbo": "^1.10.6",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.32.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
